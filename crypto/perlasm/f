sub out3
	{
	local($name,$p1,$p2,$p3)=@_;

	push(@out,"\t$name\t");
	$l=length($p1)+1;
	push(@out,$p1.",");
	$ll=4-($l+9)/8;
	$tmp1=sprintf("\t" x $ll);
	push(@out,$tmp1);

	$l=length($p2)+1;
	push(@out,$p2.",");
	$ll=4-($l+9)/8;
	$tmp1=sprintf("\t" x $ll);
	push(@out,$tmp1);

	push(@out,&conv($p3)."\n");
	}
